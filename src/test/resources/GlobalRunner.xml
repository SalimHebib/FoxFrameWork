<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Smoke Testing" thread-count="4">
    <listeners>
        <listener class-name="Listeners.Listeners"/>
    </listeners>

    <test name="Hybrid">
        <classes>
            <class name="testes.FourTest"/>
            <class name="testes.LoginTest"/>
            <class name="testes.ThreeTest"/>
            <class name="testes.TwoTest"/>
        </classes>
    </test>

    <test name="Cucumber">
        <classes>
            <class name="runner.TestRunner"/>
        </classes>
    </test>


</suite>
